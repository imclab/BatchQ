from batchq.core import batch

class SubmissionModel(batch.BatchQ):
    w = WildCard()

    param1 = Property("default value 1")
    param2 = Property("default value 2")
    .
    .
    .

    terminal1 = Controaller(Class1, arg1, arg2, ...)
    
    t1_fnc1 = Function().call1().call2()
    t1_fnc2 = Function().call1().call2()
    .
    .
    .

    terminal2 = Controaller(Class2, arg1, arg2, ...)

    t2_fnc1 = Function().call1().call2()
    t2_fnc2 = Function().call1().call2()
    .
    .
    .
