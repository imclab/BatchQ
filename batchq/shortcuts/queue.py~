def loadq(cls,filename):
    file = open(conffile)
    newargs, newkwargs, switches = json.load(file)
    u,t,i,q,f = switches
    if len(args)< len(newargs):
        for n in range(0,len(args)):
            newargs[n] = args[n]
            args = tuple(newargs)
            
    file.close()
    newkwargs.update(kwargs)
    kwargs = newkwargs

